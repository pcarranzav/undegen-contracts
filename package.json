{
  "name": "undegen-contracts",
  "version": "1.0.0",
  "description": "Smart Contracts for Undegen project at EthGlobal SF hackathon",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint:ts": "eslint '**/*.{js,ts}' --fix",
    "lint:sol": "prettier --write contracts/**/*.sol && solhint --noPrompt --fix contracts/**/*.sol --config node_modules/solhint-graph-config/index.js",
    "lint": "yarn lint:ts && yarn lint:sol",
    "clean": "rm -rf build cache typechain-types",
    "build": "forge build && hardhat compile",
    "test": "forge test && hardhat test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcarranzav/undegen-contracts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pcarranzav/undegen-contracts/issues"
  },
  "homepage": "https://github.com/pcarranzav/undegen-contracts#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-foundry": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@openzeppelin/contracts": "^5.1.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^22.7.7",
    "ethers": "^6.13.4",
    "hardhat": "^2.22.13",
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1",
    "solhint": "^5.0.3",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.6.3"
  }
}
